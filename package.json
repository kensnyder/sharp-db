{
  "name": "sharp-db",
  "version": "2.0.0",
  "description": "Classes for running SQL and building select queries for MySQL in Node",
  "main": "dist/index.js",
  "exports": {
    "require": "dist/index.js",
    "import": "dist/index.mjs"
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test-watch": "vitest",
    "coverage": "npx jest --coverage",
    "clean": "rimraf dist/ && mkdir dist && touch dist/.gitkeep",
    "build": "npm run clean && npx -p typescript tsc --project tsconfig.build.json",
    "view-coverage": "open ./coverage/lcov-report/index.html",
    "docs": "npx jsdoc --configure .jsdoc.json --verbose",
    "view-docs": "open docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/sharp-db.git"
  },
  "keywords": [
    "Db",
    "Select",
    "parse SQL",
    "SQL Parser",
    "Select Parser",
    "MySQL",
    "mysql2",
    "PostgreSQL",
    "Postgres",
    "pg",
    "ssh"
  ],
  "author": "kendsnyder@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kensnyder/sharp-db/issues"
  },
  "homepage": "https://github.com/kensnyder/sharp-db#readme",
  "dependencies": {
    "is-plain-object": "5.0.0",
    "lodash.clonedeep": "4.5.0"
  },
  "peerDependencies": {
    "mysql2": "3.x",
    "pg": "8.x",
    "ssh2": "1.x"
  },
  "peerDependenciesMeta": {
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "ssh2": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/mysql": "2.15.24",
    "@types/pg": "8.10.9",
    "@types/ssh2": "1.11.17",
    "ansi-colors": "4.1.3",
    "codecov": "3.8.3",
    "jsdoc": "4.0.2",
    "minami": "1.2.3",
    "moment": "2.29.4",
    "prettier": "3.1.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "pretty-var-export": "1.1.7",
    "prompts": "2.4.2",
    "rimraf": "5.0.5",
    "typescript": "5.3.2",
    "vitest": "0.34.6",
    "walkdir": "0.4.1"
  }
}
